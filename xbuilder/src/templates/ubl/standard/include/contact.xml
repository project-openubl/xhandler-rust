{%- macro contact(contacto) -%}
            <cac:Contact>
                {%- if contacto.telefono %}
                <cbc:Telephone>{{contacto.telefono}}</cbc:Telephone>
                {%- endif %}
                {%- if contacto.email %}
                <cbc:ElectronicMail>{{contacto.email}}</cbc:ElectronicMail>
                {%- endif %}
            </cac:Contact>
{%- endmacro contact -%}
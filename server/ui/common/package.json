{
  "name": "@openubl-ui/common",
  "description": "ESM module for code common to client and server",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production rollup -c",
    "start:dev": "NODE_ENV=development rollup -c --watch"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}": "eslint --fix",
    "*.{css,json,md,yaml,yml}": "prettier --write"
  },
  "dependencies": {
    "ejs": "^3.1.7",
    "express": "^4.20.0",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {}
}
